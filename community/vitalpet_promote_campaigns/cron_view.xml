<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_reportcard_action" model="ir.cron">
            <field name="name">Generate Report Card Cron</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field eval="True" name="active"/>
            <field eval="'res.partner'" name="model"/>
            <field eval="'edit_reportcard_cron'" name="function"/>
            <field eval="'()'" name="args"/>
            <field eval="datetime.now().replace(hour=0, minute=0, second=0, microsecond=0)" name="nextcall"/>
        </record>
   </data>

</odoo>
