<?xml version="1.0"?>
<odoo>
	<record id="account_entries_warnig_form" model="ir.ui.view">
		<field name="name">account.entries</field>
		<field name="model">account.entries</field>
		<field name="arch" type="xml">
			<form string="Set Accounts">
				
					<div>Already accounts been mapped, entering new account will replaces/updates the exisitng account(s)</div>
					<div>Do you want to continue? </div>
				
				<footer>
					<button name="continue_to_new_entry" string="Yes" type="object"
						class="btn btn-sm btn-primary" />
					<button string="Cancel" class="btn btn-sm btn-default"
						special="cancel" />
				</footer>
			</form>
		</field>
	</record>
</odoo>
	