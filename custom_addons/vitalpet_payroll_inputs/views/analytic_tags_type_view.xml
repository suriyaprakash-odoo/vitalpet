        <record model="ir.ui.view" id="analytic_tags_type_form_view">
            <field name="name">Tags</field>
            <field name="model">hr.production.type</field>
            <field name="arch" type="xml">
                <form string="Tags">
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name='color'/>
                        </group>
                    </group>
                </form>
            </field>
        </record>
        <record id="analytic_tags_type_action" model="ir.actions.act_window">
            <field name="name">Analytic Tags Type</field>
            <field name="res_model">hr.production.type</field>
            <field name="view_type">form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to add a new tag.
              </p>
            </field>
        </record>


