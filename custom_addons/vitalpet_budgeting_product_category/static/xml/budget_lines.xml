<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <!--
        This template is the ribbon at the top of the Billpay Inbox that adds
        some figures to it. We call this rubbon the "Billpay Ibox".
    -->
    <t t-name="vitalpet_budgeting_product_category.BillpayInbox">
    	<style>
.image_file{
    padding-left: 0px;
    background: #fff;
    width: 418px;
    height: 68px;
    vertical-align: middle;
}

.upload_icon{
    background: #875a7b;
    color: #fff;
    padding: 19px;
    font-size: 30px;
    float: left;
}
.drag_text{
    float: left;
    padding-top: 24px;
    padding-left: 8px;
    font-size: 16px;
}
.browse{
    background: #875a7b;
    border: 0;
    color: #fff;
    padding: 7px 17px;
    margin-left: 16px;
    float: left;
    margin-top: 18px;
    font-size: 15px;
}
.o_image_upload{
	background: #fff;
	opacity: 0;
    width: 418px;
    height: 68px;
	vertical-align: middle;
}
.oe_view_nocontent{display:none;}
	     </style>

<div style="padding-left: 4px;width: 100%;margin-bottom: 10px;display:none;">
	 <button class="browse kanban_budget_lines" >Multi Practice</button>
	 <button class="browse kanban_budget_lines_monthly" >Multi Practice</button>
	 <button class="browse kanban_budget_lines_quarterly" >Multi Practice</button>
     <button class="browse kanban_budget_lines_yearly" >Multi Practice</button>
     <button class="browse kanban_budget_lines_yearly_consolidate" >Multi Practice</button>
     <button class="browse kanban_budget_lines_quarterly_consolidate" >Multi Practice</button>
     <button class="browse kanban_budget_lines_monthly_consolidate" >Multi Practice</button>
</div>
    </t> 
