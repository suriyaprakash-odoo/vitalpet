	<record model="ir.ui.view" id="update_img_and_notes">
		<field name="name">Update Notes and Image</field>
		<field name="model">update.image</field>
		<field name="arch" type="xml">
			<form>
				<group>
					<field name="notes" />
					<field name="image" />
				</group>
				<footer>
					<button string='Update' name="update_image_notes" type="object"
						class="btn-primary"/>
					<button string="Cancel" class="btn-default" special="cancel" />
				</footer>
			</form>
		</field>
	</record>
	
	
	<record id="update_confirmation_warning" model="ir.ui.view">
			<field name="name">Update confirmation</field>
			<field name="model">update.confirmation</field>
			<field name="arch" type="xml">
				<form>

					<div>Already Values have been updated.</div>
					<div>Do you want to replace it? </div>

					<footer>
						<button name="continue_to_update" string="Yes"
							type="object" class="btn btn-sm btn-primary" />
						<button string="Cancel" class="btn btn-sm btn-default"
							special="cancel" />
					</footer>
				</form>
			</field>
		</record>
		
	<record id="update_image_and_notes" model="ir.actions.act_window">
		<field name="name">Update Image and Notes</field>
		<field name="res_model">update.image</field>
		<field name="src_model">update.image</field>
		<field name="type">ir.actions.act_window</field>
		<field name="view_type">form</field>
		<field name="view_mode">form</field>
		<field name="target">new</field>
	</record>
	
	<record id="custom_my_practice_project_filter" model="ir.ui.view">
        <field name="name">project.project.select</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.view_project_project_filter"/>
        <field name="arch" type="xml">
            <filter name="my_projects" position="after">
            <separator/>
            <filter name="mypractice" string="Not My Practice" domain="[('mypractice', '=', False)]" />
            </filter>
        </field>
	</record>
        
    </data>
</odoo>
