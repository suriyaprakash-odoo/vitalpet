    <record id="practice_hr_appraisal_search" model="ir.ui.view">
        <field name="name">hr.appraisal.search</field>
        <field name="model">hr.appraisal</field>
        <field name="arch" type="xml">
            <search string="Search Appraisal">
                <field name="employee_id"/>
                <field name='company_id'/>

                <filter string="Archived" name="inactive" domain="[('active','=',False)]"/>
                <group expand='0' string='Group by...'>
                    <filter string='Employee' icon="fa-user" domain="[]" context="{'group_by' : 'employee_id'}"/>
                    <filter string='Company' icon="fa-user" domain="[]" context="{'group_by' : 'company_id'}" name='company'/>
                    <filter icon="terp-go-month" string="Today " separator="1" name='today'
                        domain="[('date_close','&lt;=', (datetime.date.today()).strftime('%Y-%m-%d')),('date_close','&gt;=',(datetime.date.today()).strftime('%Y-%m-%d'))]"/>

                    <filter icon="terp-go-month" string="Next 2 Weeks" separator="1" name='next2week'
                        domain="[('date_close','&gt;', (datetime.date.today().strftime('%Y-%m-%d'))),('date_close','&lt;=', ((context_today()+datetime.timedelta(days=13)).strftime('%Y-%m-%d')))]"/>

                    <filter icon="terp-go-month" string="Expired" separator="1" name='expired'
                        domain="[('date_close','&lt;=', (datetime.date.today().strftime('%Y-%m-%d')))]"/>



               </group>
           </search>
        </field>
    </record>

